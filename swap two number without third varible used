

class HelloWorld {
    public static void main(String[] args) {
       int a=10;
       int b=30;
       System.out.println("swap these without using third character a & b");
       System.out.println(a);
       System.out.println(b);
       b=a+b;
       a=b-a;
       b=b-a;
       System.out.println("swapped numbers are a & b");
              System.out.println(a);
       System.out.println(b);

    }
}
